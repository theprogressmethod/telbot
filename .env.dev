# Development Environment Variables for @TPM_superbot
# Copy this to .env and replace with your actual values

# @TPM_superbot Bot Token (get from @BotFather)
BOT_TOKEN=your_superbot_token_here

# Supabase (can use same database or separate dev database)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# OpenAI API Key (same as production is fine)
OPENAI_API_KEY=your_openai_key_here

# Development Settings
ENVIRONMENT=development
DEBUG=true